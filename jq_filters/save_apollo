._source | with_entries(. as $value | ("b_firstname", "b_lastname", "b_address", "b_city", "b_state", "b_country", "b_zipcode", "b_phone", "s_phone", "person_location_city", "person_location_state", "email", "organization_facebook_url", "organization_twitter_url") as $desired | if ($desired | index($value.key)) then (. | select(.value != "")) else empty end)
