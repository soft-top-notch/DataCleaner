._source | with_entries(. as $value | ("person_name", "person_title", "person_email", "person_phone", "person_sanitized_phone", "person_linkedin_url", "organization_name", "organization_website_url", "organization_domain", "person_location_city", "person_location_state", "person_location_country", "organization_facebook_url", "organization_twitter_url") as $desired | if ($desired | index($value.key)) then (. | select(.value != "")) else empty end)
