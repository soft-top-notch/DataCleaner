with_entries(
  if (.value | type) == "object" then
    .value = (
      .value |
      if .address then
        .a = .address
      else
        .
      end |
      if .a2 then
        if .a then
          .a = .a + " " + .a2
        else
          .a = .a2
        end
      else
        .
      end |
      if .a3 then
        if .a then
          .a = .a + " " + .a3
        else
          .a = .a3
        end
      else
        .
      end |
      if .a4 then
        if .a then
          .a = .a + " " + .a4
        else
          .a = .a4
        end
      else
        .
      end |
   if .a5 then
        if .a then
          .a = .a + " " + .a5
        else
          .a = .a5
        end
      else
        .
      end |
      if .fn then
        .n = .fn
      else
        .
      end |
      if .ln then
        if .n then
          .n = .n + " " + .ln
        else
          .n = .ln
        end
      else
        .
      end |
      with_entries(
        if .key == "d" then
          {"key": "dob", "value": .value}
        else
          .
        end |
        select(
          .value != "" and
          .value != "null" and
          .value != "null null" and
	  .value != " " and
          (.key | test("x\\d+$") | not) and
          .key != "fn" and
          .key != "ln" and
          .key != "a2" and
          .key != "a3" and
          .key != "a4" and
          .key != "a5" and
          (.value | test("^,+$") | not))) |
          if .e then
            .d = (.e | split("@")[1])
          else
            .
          end
        )
  else
    .
  end
)
